{
  "name": "ng2-material-dropdown",
  "version": "0.11.0",
  "description": "Angular material-like dropdown component",
  "scripts": {
    "release": "npm run build && npm publish dist",
    "postrelease": "rm -rf dist",
    "build": "ng-packagr -p package.json && mkdir ./dist/dist && cp ./dist/bundles/ng2-material-dropdown.umd.js ./dist/dist/ng2-dropdown.bundle.js",
    "server": "webpack-dev-server --config webpack.demo.js --inline --colors --progress --display-error-details --display-cached --port 3005  --content-base demo",
    "start": "npm run server",
    "test": "karma start karma.conf.js --single-run"
  },
  "author": {
    "name": "Giancarlo Buomprisco",
    "email": "giancarlopsk@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-shim": "0.31.37",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^10.12.0",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^8.4.1",
    "awesome-typescript-loader": "4.0.0",
    "core-js": "^2.5.7",
    "css-loader": "^0.28.11",
    "es6-promise": "4.2.5",
    "es6-shim": "0.35.3",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "jasmine": "^2.5.3",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "ng-packagr": "^4.4.0",
    "node-sass": "^4.9.4",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "protractor": "^5.4.1",
