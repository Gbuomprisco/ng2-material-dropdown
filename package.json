    "svg-url-loader": "^1.0.3",
    "ts-helpers": "1.1.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "typings": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-server": "https://registry.npmjs.org/webpack-dev-server/-/webpack-dev-server-2.11.1.tgz",
    "webpack-merge": "^4.1.0",
    "zone.js": "0.8.26"
